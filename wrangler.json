{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "tg-voice-ai",
	"main": "src/index.ts",
	"compatibility_date": "2025-01-21",
	"observability": {
		"enabled": true
	},
	"ai": {
		"binding": "AI"
	},
	"compatibility_flags": [
		"nodejs_compat"
	],
	"kv_namespaces": [
		{
			"binding": "tgvoicechat",
			"id": "0f1d9b70c84544a18a83bd2b90d664fd"
		}
	],
	"vars": {
		"tts_speaker": "asteria",
		"tts_encoding": "mp3",
		"tts_container": "mp3",
		"tts_provider": "deepgram"
	}
}
